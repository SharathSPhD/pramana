# Pramana Environment Configuration
# Copy to .env and fill in your values

# HuggingFace Hub
HF_TOKEN=your_huggingface_token_here
HF_HOME=/workspace/pramana/hf_cache

# Weights & Biases
WANDB_PROJECT=pramana
WANDB_API_KEY=your_wandb_api_key_here

# OpenAI (for Tier 2 LLM Judge)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic (alternative for Tier 2 LLM Judge)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Pramana Configuration
PRAMANA_DATA_DIR=/workspace/pramana/data
PRAMANA_MODELS_DIR=/workspace/pramana/models
PRAMANA_LOG_LEVEL=INFO
